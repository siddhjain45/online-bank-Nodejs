<html lang="en">
    <head >
        <meta charset="utf-8">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.1/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-+0n0xVW2eSR5OomGNYDnhzAbDsOXxcvSN1TPprVMTNDbiYZCxYbOOl7+AMvyTG2x" crossorigin="anonymous">    
        <title>Admin Login </title>
    
</head>

<body style="background-color:white; margin:0;padding:0;">
    
    

        <div class="container-fluid "  style="height:250px; background-image:url(./image/globe.gif)">
            <br><br><br>
            
                <h1 class="text-center text-light" style="font-size: 70px; font-family: fantasy;">Indore Union Bank</h1>         
            </div>
            <nav class="navbar navbar-expand-lg navbar-dark " style="background-color:black;">
                <div class="container-fluid " style="background-color:black;">
                    <div class="collapse navbar-collapse" id="navbarSupportedContent">
                        <ul class="navbar-nav me-auto mb-2 mb-lg-0" >
                            <li class="nav-item">
                                <a class="nav-link active text-light text-lg-center" aria-current="page" href="/home">Home</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link text-light" href="/admin">Manager Login</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link text-light" href="/user">User Login</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link text-light" href="#">Contact us</a>
                            </li>
                          
                          
                    </div>
                </div>
            </nav>
            <marquee direction="left" >
                <div class="container bg-info">
                  <h2 class="text-center">Education Loan at 12%  Housing loan at 6.5%</h2>
                </div>
            </marquee>

            <%if(err>0){%>
                <center>
                    <div class="alert alert-danger alert-dismissible fade show" role="alert" style="width: 600px;">
                        Invalid Username or password !!
                         <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
                       </div>
                     </center>
                <%} %>
             
        <table>
            <tr>
                <td class="container" style="width: 410px;"></td>
                <td>
                    <div class="container my-5 m-lg-5">
                        <div class="card shadow-lg p-3 mb-5 bg-body rounded " style="width: 25rem;">
                            <div class="card-body">
                                <h4 class="card-title text-dark my-2" style="text-align: center;">Manager Login</h4>
                                <main class="form-signin my-4">
                                    <form action="/adminlogin" method="POST">
    
                                        <div class="form-floating">
                                            <input type="text" class="form-control" id="floatingInput" autocomplete="off" placeholder="name@example.com" name="uname">
                                            <label for="floatingInput">Enter Username</label>
                                        </div>
                                        <div class="form-floating">
                                            <input type="password" class="form-control" id="floatingPassword"  placeholder="Pin" name="pass">
                                            <label for="floatingPassword">Enter Password</label>
                                        </div>
    
                                       <br>
                                        <button class="w-100 btn btn-lg btn-warning" type="submit">Login</button>
                                        
                                    </form>
                                </main>
                            </div>
                        </div>
                    </div>
                </td>
            </tr>
        </table>
          
 
